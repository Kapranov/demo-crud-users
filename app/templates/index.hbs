<div class="main">
  <div class="tools">
    <a class="button-short blue create-btn" href="#">
      <i class="fa fa-user"></i>
      <span>⬅ </span>
    </a>
    <div class="users-count">
      Users: {{model.length}}
    </div>
  </div>
  <ul class="users-listing">
    {{#each model as |user|}}
      <li>{{user.name}}</li>
    {{else}}
      <li>no users… :-(</li>
    {{/each}}
  </ul>
  <div class="user-pane"></div>
</div>
